{
  "name": "webshopapps/mage2-matrixrate",

  "description": "Magento Shipping Multiple Table Rates - MatrixRate",

  "license": "OSL",

  "authors": [
    {
      "name": "WebShopApps",
      "email": "sales@webshopapps.com",
      "homepage": "http://www.WebShopApps.com"
    },
    {
      "name": "Karen Baker"
    }
  ],

  "type": "magento2-module",

  "version": "0.1.0",

  "support": {
    "source": "https://github.com/webshopapps/mage2-matrixrate",
    "issues": "https://github.com/webshopapps/mage2-matrixrate/issues"
  },

  "autoload": {
    "psr-4": {
      "Matrixrate\\": "src/"
    }
  },

  "autoload-dev": {
    "psr-4": {
      "Matrixrate\\Test\\Unit\\": "tests/unit/",
      "Matrixrate\\Test\\Functional\\": "tests/functional/"
    }
  },

  "require": {
    "php": ">=5.4.0",
    "magento/framework": "*" // initially tested on 0.42.0-beta3
  },

  "require-dev": {
    "mockery/mockery": "~0.9",
    "phpunit/phpunit": "~4.0",
    "sensiolabs/security-checker": "~2.0",
    "squizlabs/php_codesniffer": "~1.0"
  },

  "suggest": {
    "sensiolabs/security-checker": "Required for ComposerSecurityReview.",
    "squizlabs/php_codesniffer":   "Required for PhpCodeSnifferReview."
  },

  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist"
  }
}
